z1 = (0,3/4h); z2=(w,3/4h); draw z1..z2;
z3 = (1/2w,5/9h); lft z4 = (6/7w,7/20h);
z5 = (3/5w,1/12h); rt z6 = (1/10w,13/20h); z7 = (1/5w,1/3h);
pickup pencircle xscaled 0.3pt yscaled 0.6pt;
draw z3{dir 28}..{down}z4{down}..{left}z5{left}..z7..z6;
