z5 = (w-0.9pt,13/20h);
z5r = z5 + (0,penth#/2); z5l = z5 - (0,penth#/2);
z6 = (1/8w,8/21h);
numeric len; len = 1/2(penth#/(sqrt 2));
z6l = z6 + (len,-len); z6r = z6 - (len,-len);
penstroke z5e{z6-z5 rotated -8}..z6e;
bot z4 = (w-0.9pt,0);
draw z6{dir -20}..z4;
top z3 = (w-0.9pt,3/4h);
draw z3..z4;
fill fullcircle scaled 1.2pt shifted (2/5w,1/20h);
endchar;

